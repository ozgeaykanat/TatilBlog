@using TravelTrip.Models.Sınıflar
@model List<Blog>

<div class="padding">
<h4>En Popüler Bloglar</h4>
<ul class="list list-pad">
    @{
        int sayac = 1;
    }
    @foreach (var x in Model)
    {
        <li class="list-bg"><span>@sayac.</span><a href="/Blog/BlogDetay/@x.ID">@x.Baslik</a></li>
        sayac++;
    }

</ul>
 <a href="/Blog/Index/" class="link1">Tüm Blogları Gör</a>
</div>